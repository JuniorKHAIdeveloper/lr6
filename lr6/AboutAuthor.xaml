<Page
    x:Class="lr6.AboutAuthor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:lr6"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="3*"></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="button_back" Content="Назад" FontSize="10" Click="button_back_Click"/>
        <TextBlock HorizontalAlignment="Center" Grid.Row="1" Text="Іванов Андрій Геннадійович. гр.535 ст2" VerticalAlignment="Top"/>
    </Grid>
</Page>
